<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


 <!-- Setter method dependency Injection -->

	<bean class="in.sp.beans.Address" id = "addrId">
		<property name="houseno" value="21" />
		<property name="city" value="Nashik" />
		<property name="pincode" value="411033" />
	</bean>

	<bean class="in.sp.beans.Student" id = "stdId">
		<property name="rollno" value="93" />
		<property name="name" value="Pranit" /> <!--property tag internally calling setter method  -->
		<property name="address" ref="addrId" />  <!--To pass object we need to pass Reference  -->
	</bean>

</beans>
